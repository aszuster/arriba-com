{
  "name": "cotd",
  "version": "0.0.3",
  "private": true,
  "devDependencies": {
    "concurrently": "6.4.0",
    "react-scripts": "4.0.3",
    "sass": "^1.49.7"
  },
  "dependencies": {
    "autoprefixer-stylus": "1.0.0",
    "bootstrap": "^5.1.3",
    "firebase": "8.10.0",
    "gsap": "^3.10.2",
    "prop-types": "15.7.2",
    "re-base": "4.0.0",
    "react": "17.0.2",
    "react-animated-css": "^1.2.1",
    "react-dom": "17.0.2",
    "react-easy-marquee": "^1.2.3",
    "react-helmet": "^6.1.0",
    "react-preloaders": "^3.0.3",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "5.3.0",
    "react-select": "^5.2.2",
    "react-slick": "^0.28.1",
    "react-spinners": "^0.11.0",
    "react-spring": "^9.4.4",
    "react-transition-group": "4.4.2",
    "reactstrap": "^9.0.1",
    "serve": "13.0.2",
    "slick-carousel": "^1.8.1",
    "stylus": "0.55.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.scss -o ./src/css/style.css",
    "now-build": "npm run build && mv build dist",
    "sass": "sass src/sass:src/css --watch --no-source-map",
    "styles:watch": "npm run styles -- -w"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
